.line-text-line-tpl {
  background: #EFEFF4;
  margin: 30rpx 0;
  text-align: center;
}
.line-text-line-tpl .line {
  background: #D8D8D8;
  margin-top: 4rpx;
  display: inline-block;
  width: 100rpx;
  height: 1rpx;
  vertical-align: middle;
}
.line-text-line-tpl .title {
  display: inline-block;
  background: #EFEFF4;
  padding: 0 30rpx;
  position: relative;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  color: #888888;
}
.blank-signal-tpl {
  background: #EFEFF4;
  text-align: center;
  margin-top: 140rpx;
}
.blank-signal-tpl.detail {
  margin-top: 0;
}
