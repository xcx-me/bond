<view class='wx-dialog-container' hidden="{{!isShow}}">
	<view class='wx-dialog-mask'></view>
	<view class='wx-dialog-main'>
		<!-- <view class='wx-dialog-title'>{{ title }}</view> -->
		<view class='wx-dialog-content'>{{ content }}</view>
		<view class='wx-dialog-footer'>
			<view class='btn btn-cancel' catchtap='_cancelEvent'>{{ cancelText }}</view>
			<view class='btn btn-confirm' catchtap='_confirmEvent'>{{ confirmText }}</view>
		</view>
	</view>
</view>