<loading wx:if='{{loading}}'>加载中...</loading>
<view class='administration-page' hidden='{{loading}}'>
	<block wx:if='{{!isStoreRegistered}}'>
			<StoreRegister type='page' />
		</block>
		<block wx:else>
			<view class='to-quotation' bindtap='createQuotation'>
				<image src='../../asset/image/qtrade/release_36.svg' />
				<text>发布报价</text>
				<i class='icon-right-arrow' />
			</view>
			<view hidden='{{loading}}' class='bond-list-pallet'>
				<BondList from='admin'
					isMine='{{true}}'
					bondList='{{bondList}}'
					uid='0'
					userId='' 
					bind:modifyBondEvent='onModifyBondEvent'
					bind:deleteBondEvent='onDeleteBondEvent'/>

					<view class="loading-more" hidden="{{!isLoadingMore}}">
    					<view class="icon"></view>
    					<view class="tips"><text>正在加载...</text></view>
  					</view>
			</view>
	</block>
</view>