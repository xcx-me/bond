.administration-page {
  padding-top: 20rpx;
  height: calc(100% - 40rpx);
  position: relative;
}
.administration-page .to-quotation {
  position: relative;
  background: #FFFFFF;
  height: 88rpx;
  line-height: 88rpx;
  padding: 0 30rpx;
}
.administration-page .to-quotation text {
  font-size: 34rpx;
}
.administration-page .to-quotation image {
  width: 36rpx;
  height: 36rpx;
  position: relative;
  top: 5rpx;
  margin-right: 20rpx;
}
.administration-page .to-quotation .icon-right-arrow {
  display: inline-block;
  width: 16rpx;
  height: 16rpx;
  border-top: 4rpx solid #C7C7CC;
  border-right: 4rpx solid #C7C7CC;
  transform: rotate(45deg);
  position: absolute;
  top: 35rpx;
  right: 30rpx;
}
.administration-page .bond-list-pallet {
  width: 100%;
  height: calc(100% - 44rpx);
}
