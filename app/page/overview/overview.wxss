.seller-info-tpl {
  overflow: hidden;
}
.seller-info-tpl view {
  float: left;
}
.seller-info-tpl .face-url {
  width: 60rpx;
  height: 60rpx;
  border-radius: 100%;
  position: relative;
  left: 20rpx;
}
.seller-info-tpl .v-user {
  position: relative;
  top: 12rpx;
  left: -2rpx;
  padding: 18rpx;
  background-size: 90rpx;
}
.seller-info-tpl .v-user.big {
  background-size: 90rpx;
  background-position: 0 -1120px;
}
.seller-info-tpl .under-line {
  font-size: 0;
}
.seller-info-tpl .user-name {
  font-size: x-large;
}
.seller-info-tpl .company-name {
  font-size: small;
}
.overview-page {
  height: 100%;
}
.overview-page .pallet {
  padding: 10rpx;
  background: #FFFFFF;
}
.overview-page .pallet-has-slite-line {
  border-top: 1rpx solid #CCCCCC;
}
.overview-page .mystore-pallet {
  font-size: normal;
  padding-bottom: 10rpx;
}
.overview-page .mystore-pallet navigator {
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 20rpx;
  cursor: pointer;
}
.overview-page .dynamic-pallet {
  padding: 20rpx;
}
.overview-page .recent-notice {
  margin-top: 20rpx;
}
.overview-page .recent-notice .register-store {
  text-align: center;
}
.overview-page .recent-notice .register-store .tips {
  height: 200rpx;
  position: relative;
}
.overview-page .recent-notice .register-store .tips text {
  position: relative;
  top: 40%;
}
.overview-page .recent-notice .shop-button {
  background-color: #2196F3;
}
.overview-page .recent-notice .button-hover {
  background-color: #1A7DD4;
}
.overview-page .recent-notice .protocol {
  font-size: small;
  position: relative;
  margin-top: 20rpx;
  height: 100%;
  width: 100%;
  display: flex;
  justify-content: center;
}
.overview-page .recent-notice .protocol checkbox {
  position: relative;
  top: -2px;
}
.overview-page .recent-notice .protocol label {
  width: 159px;
  display: flex;
}
.overview-page .recent-notice .protocol .to-qtrade-protocol {
  color: #2196F3;
  text-decoration: underline;
}
