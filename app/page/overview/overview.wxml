<view class='overview-page'>
  <view class='summary'>
    <view class='name-pallet'>
      <navigator url='../my-configuration/my-configuration'>
        <text class='person-name'>刘明</text>
        <text class='agency-name'>苹果银行</text>
      </navigator>
    </view>
  </view>
  <view class='recent-notice'>
    <view wx:if='{{storeRegistered}}'>
      <navigator url='../my-store/my-store'>进入我的店铺</navigator>
    </view>
    <view wx:else>
      <button type='primary' bindtap='registerStore'>立马开店</button>
    </view>
  </view>
</view>