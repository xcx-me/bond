/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
page {
  height: 100%;
  background-color: #EFEFF4;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.clearfix {
  zoom: 1;
}
.clearfix::after {
  content: "";
  display: block;
  clear: both;
  height: 0;
}
.btn-no-border {
  background: none;
  border: none;
  outline: none;
  text-decoration: underline;
  color: #2196F3;
}
