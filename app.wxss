/**app.wxss**/
.container {
	height: 100%;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: space-between;
	padding: 200rpx 0;
	box-sizing: border-box;
}

page {
	height: 100%;
	background-color:#F1F1F1;
}

page .page-wrapper {
	padding: 20rpx 20rpx 0 20rpx;
}

::-webkit-scrollbar {
	width: 0;
	height: 0;
	color: transparent;
}
	